# Decksnap Environment Configuration
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql+asyncpg://decksnap:decksnap@localhost:5432/decksnap

# OpenRouter API (for slide generation)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=@preset/pup

# Application
DEBUG=true
CORS_ORIGINS='["http://localhost:13000"]'

# Frontend API URL (for Next.js)
NEXT_PUBLIC_API_URL=http://localhost:18000
