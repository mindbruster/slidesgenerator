slides_api: powershell -Command "$env:DATABASE_URL = 'postgresql+asyncpg://decksnap:decksnap@localhost:5432/decksnap'; $env:REDIS_URL = 'redis://localhost:6379'; python -m poetry run uvicorn apps.slides_api.main:app --host 0.0.0.0 --port 18000 --reload"
public_api: powershell -Command "$env:DATABASE_URL = 'postgresql+asyncpg://decksnap:decksnap@localhost:5432/decksnap'; $env:REDIS_URL = 'redis://localhost:6379'; python -m poetry run uvicorn apps.public_api.main:app --host 0.0.0.0 --port 19000 --reload"
frontend: powershell -Command "Set-Location -LiteralPath 'apps/frontend'; npm run dev -- --port 13000"
